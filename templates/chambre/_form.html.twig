
<div class="container register-form">
    <div class="form">
        <h2> <a href="{{ path('chambre_index') }}">liste de Chambres</a></h2>
        <div class="note">

            <h1>Creation des chambres</h1>
        </div>

        <div class="form-content">

            <div class="row">

                <div class="col-md-6">
                    {{ form_start(form) }}
                    <div class="form-group">
                        {{ form_row(form.numero_chambre) }}
                    </div>
                    <div class="form-group">
                        {{ form_row(form.type) }}
                    </div>
                </div>

            </div>
            <button class="btn btn-success">{{ button_label|default('Enregistere') }}</button>
            {{ form_end(form) }}
        </div>
    </div>
</div>
